
def pair x y
	LD 0 0
	LD 0 1
	CONS
end

def element board row col
	return list (list board row) col
end

def pill
	LDC 2
end

def equal a b
	LD 0 0
	LD 0 1
	CEQ
end

def list l x
	if zero x
	   first l 
	else
	   list (second l) (dec x)
	endif
end

def zero x
	LD 0 0
	LDC 0
	CEQ
end

def first l
	LD 0 0
	CAR
end

def second l
	LD 0 0
	CDR
end

def dec x
	LDC 1
	LD 0 0
	SUB
end

def inc x
	LDC 1
	LD 0 0
	ADD
end 